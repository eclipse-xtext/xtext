/*
* generated by Xtext
*/
grammar DebugInternalIndentationAwareTestLanguage ;

// Rule Tree
ruleTree :
	ruleTreeNode*
;

// Rule TreeNode
ruleTreeNode :
	RULE_ID (
		RULE_INDENT ruleTreeNode* RULE_DEDENT
	)?
;

RULE_INDENT :
	'synthetic:INDENT'
;

RULE_DEDENT :
	'synthetic:INDENT'
;

RULE_ID :
	'^'? (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_'
	) (
		'a' .. 'z' |
		'A' .. 'Z' |
		'_' |
		'0' .. '9'
	)*
;

RULE_INT :
	'0' .. '9'+
;

RULE_STRING :
	'"' (
		'\\' . |
		~ (
			'\\' |
			'"'
		)
	)* '"' |
	'\'' (
		'\\' . |
		~ (
			'\\' |
			'\''
		)
	)* '\''
;

RULE_ML_COMMENT :
	'/*' (
		options { greedy = false ; } : .
	)* '*/' {skip();}
;

RULE_SL_COMMENT :
	'//' ~ (
		'\n' |
		'\r'
	)* (
		'\r'? '\n'
	)? {skip();}
;

RULE_WS :
	(
		' ' |
		'\t' |
		'\r' |
		'\n'
	)+ {skip();}
;

RULE_ANY_OTHER :
	.
;